{
  "name": "@ovotech/laminar-oapi-cli",
  "version": "0.5.6",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "CLI generating typescript types for @ovotech/laminar-oapi",
  "author": "Ivan Kerin <ikerin@gmail.com>",
  "repository": "git@github.com:ovotech/laminar.git",
  "homepage": "https://github.com/ovotech/laminar/tree/master/packages/laminar-oapi-cli#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ovotech/laminar": "^0.6.1",
    "@ovotech/laminar-oapi": "^0.4.2",
    "@types/jest": "^25.2.3",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "^14.0.6",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "axios": "^0.19.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.0.1",
    "pg": "^8.2.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "bin": {
    "laminar-oapi": "./scripts/laminar-oapi"
  },
  "scripts": {
    "laminar-oapi": "node -r ts-node/register src/cli.ts",
    "test:build:integration": "yarn laminar-oapi test/integration.yaml test/__generated__/integration.ts",
    "test:build:statements": "yarn laminar-oapi test/statements.yaml test/__generated__/statements.ts",
    "build:examples": "yarn laminar-oapi examples/oapi.yaml examples/oapi.yaml.ts",
    "pretest": "yarn test:build:integration && yarn test:build:statements",
    "build": "tsc --declaration",
    "build:docs": "yarn build:examples && build-docs README.md",
    "test": "jest test --runInBand",
    "lint:prettier": "prettier --list-different '{src,test}/**/*.ts'",
    "lint:eslint": "eslint '{src,test}/**/*.ts'",
    "lint": "yarn lint:prettier && yarn lint:eslint"
  },
  "jest": {
    "preset": "../../jest.json"
  },
  "peerDependencies": {
    "@ovotech/laminar": "*"
  },
  "dependencies": {
    "@ovotech/ts-compose": "^0.14.0",
    "commander": "^5.1.0",
    "openapi-schemas": "^2.0.3",
    "openapi3-ts": "^1.3.0"
  }
}
